{
  "title": "ML Model Performance",
  "tags": [
    "ml",
    "model",
    "streamlit"
  ],
  "timezone": "browser",
  "schemaVersion": 16,
  "version": 0,
  "refresh": "10s",
  "panels": [
    {
      "id": 1,
      "title": "Model Accuracy",
      "type": "stat",
      "gridPos": {
        "h": 8,
        "w": 6,
        "x": 0,
        "y": 0
      },
      "targets": [
        {
          "expr": "avg(streamlit_model_accuracy) * 100",
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "decimals": 1,
          "min": 0,
          "max": 100
        }
      }
    },
    {
      "id": 2,
      "title": "Model Precision",
      "type": "stat",
      "gridPos": {
        "h": 8,
        "w": 6,
        "x": 6,
        "y": 0
      },
      "targets": [
        {
          "expr": "avg(streamlit_model_precision) * 100",
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "decimals": 1,
          "min": 0,
          "max": 100
        }
      }
    },
    {
      "id": 3,
      "title": "Model Recall",
      "type": "stat",
      "gridPos": {
        "h": 8,
        "w": 6,
        "x": 12,
        "y": 0
      },
      "targets": [
        {
          "expr": "avg(streamlit_model_recall) * 100",
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "decimals": 1,
          "min": 0,
          "max": 100
        }
      }
    },
    {
      "id": 4,
      "title": "Total Predictions",
      "type": "stat",
      "gridPos": {
        "h": 8,
        "w": 6,
        "x": 18,
        "y": 0
      },
      "targets": [
        {
          "expr": "sum(streamlit_predictions_total)",
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        }
      }
    },
    {
      "id": 5,
      "title": "Prediction Rate",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 8
      },
      "targets": [
        {
          "expr": "(increase(streamlit_predictions_total[15m]) / 15) or vector(0)",
          "legendFormat": "Predictions/sec",
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops"
        }
      }
    },
    {
      "id": 6,
      "title": "Prediction Batch Sizes",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 8
      },
      "targets": [
        {
          "expr": "(increase(streamlit_predictions_batch_size_sum[15m]) / increase(streamlit_predictions_batch_size_count[15m])) or vector(0)",
          "legendFormat": "Avg Batch Size",
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        }
      }
    },
    {
      "id": 7,
      "title": "Streamlit App Uptime",
      "type": "stat",
      "gridPos": {
        "h": 8,
        "w": 6,
        "x": 0,
        "y": 16
      },
      "targets": [
        {
          "expr": "max(streamlit_uptime_seconds)",
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s"
        }
      }
    },
    {
      "id": 8,
      "title": "Custom: Prediction Latency (p95)",
      "type": "stat",
      "gridPos": {
        "h": 8,
        "w": 6,
        "x": 6,
        "y": 16
      },
      "targets": [
        {
          "expr": "(histogram_quantile(0.95, sum(rate(streamlit_prediction_duration_seconds_bucket[15m])) by (le)) or vector(0))",
          "legendFormat": "p95 Latency",
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "decimals": 3
        }
      },
      "description": "Custom metric: 95th percentile prediction latency"
    },
    {
      "id": 9,
      "title": "Prediction Latency Distribution",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 16
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum(rate(streamlit_prediction_duration_seconds_bucket[15m])) by (le)) or vector(0)",
          "legendFormat": "p50",
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(streamlit_prediction_duration_seconds_bucket[15m])) by (le)) or vector(0)",
          "legendFormat": "p95",
          "refId": "B",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        },
        {
          "expr": "histogram_quantile(0.99, sum(rate(streamlit_prediction_duration_seconds_bucket[15m])) by (le)) or vector(0)",
          "legendFormat": "p99",
          "refId": "C",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "decimals": 3
        }
      }
    },
    {
      "id": 10,
      "title": "Streamlit Errors",
      "type": "stat",
      "gridPos": {
        "h": 8,
        "w": 6,
        "x": 18,
        "y": 16
      },
      "targets": [
        {
          "expr": "sum(streamlit_errors_total)",
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        }
      }
    }
  ]
}
